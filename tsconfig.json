  "compilerOptions": {
    "target": "es2017",
    "strict": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "preserveConstEnums": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "incremental": true,
    "tsBuildInfoFile": "./built/cache/tsbuildinfo"
  },
  "include": [
    "./src/**/*.ts"
  ]
}
